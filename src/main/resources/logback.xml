<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level: %msg%n</pattern>
        </encoder>
    </appender>
    <root level="trace">
        <appender-ref ref="debug"/>
    </root>
</configuration>